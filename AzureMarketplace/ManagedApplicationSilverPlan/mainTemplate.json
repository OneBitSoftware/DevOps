        {
            "condition": "[equals(parameters('vmStorageAccountNewOrExisting'),'new')]",
            "type": "Microsoft.Storage/storageAccounts",
            "apiVersion": "2022-09-01",
            "name": "[parameters('vmStorageAccountName')]",
            "location": "[parameters('location')]",
            "kind": "[parameters('vmStorageAccountKind')]",
            "sku": {
                "name": "[parameters('vmStorageAccountType')]"
            },
            "properties": {
                "accessTier": "Hot"
            }
        },
        {
            "condition": "[equals(parameters('applicationStorageAccountNewOrExisting'),'new')]",
            "type": "Microsoft.Storage/storageAccounts",
            "apiVersion": "2022-09-01",
            "name": "[parameters('applicationStorageAccountName')]",
            "location": "[parameters('location')]",
            "kind": "[parameters('applicationStorageAccountKind')]",
            "sku": {
                "name": "[parameters('applicationStorageAccountType')]"
            },
            "properties": {
                "accessTier": "Hot"
            }
        },
        {
            "type": "Microsoft.Storage/storageAccounts/fileServices/shares",
            "apiVersion": "2022-09-01",
            "name": "[format('{0}/default/{1}', parameters('applicationStorageAccountName'), concat('qdms-', take(replace(newGuid(), '-', ''), 12)))]",
            "dependsOn": [
                "[resourceId('Microsoft.Storage/storageAccounts', parameters('applicationStorageAccountName'))]"
            ]
        },
        {
            "type": "microsoft.insights/actionGroups",
            "apiVersion": "2023-01-01",
            "name": "[parameters('recommendedAlertRulesActionGroupName')]",
            "location": "Global",
            "properties": {
                "groupShortName": "recalert1",
                "enabled": true,
                "emailReceivers": [
                    {
                        "name": "Email_-EmailAction-",
                        "emailAddress": "[parameters('alertsEmail')]",
                        "useCommonAlertSchema": true
                    }
                ]
            }
        },
        {
            "type": "Microsoft.Network/networkSecurityGroups",
            "apiVersion": "2023-05-01",
            "name": "[parameters('networkSecurityGroupName')]",
            "location": "[parameters('location')]",
            "properties": {
                "securityRules": [
                    {
                        "name": "SSH",
                        "id": "[resourceId('Microsoft.Network/networkSecurityGroups/securityRules', parameters('networkSecurityGroupName'), 'SSH')]",
                        "type": "Microsoft.Network/networkSecurityGroups/securityRules",
                        "properties": {
                            "protocol": "TCP",
                            "sourcePortRange": "*",
                            "destinationPortRange": "22",
                            "sourceAddressPrefix": "*",
                            "destinationAddressPrefix": "*",
                            "access": "Allow",
                            "priority": 300,
                            "direction": "Inbound"
                        }
                    },
                    {
                        "name": "HTTPS",
                        "id": "[resourceId('Microsoft.Network/networkSecurityGroups/securityRules', parameters('networkSecurityGroupName'), 'HTTPS')]",
                        "type": "Microsoft.Network/networkSecurityGroups/securityRules",
                        "properties": {
                            "protocol": "TCP",
                            "sourcePortRange": "*",
                            "destinationPortRange": "443",
                            "sourceAddressPrefix": "*",
                            "destinationAddressPrefix": "*",
                            "access": "Allow",
                            "priority": 320,
                            "direction": "Inbound"
                        }
                    }
                ]
            }
        },
