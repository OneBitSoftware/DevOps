        "vmStorageAccountResourceGroup": {
            "defaultValue": "",
            "type": "String"
        },
        "applicationStorageAccountResourceGroup": {
            "defaultValue": "",
            "type": "String"
        },
        "applicationStorageAccountNewOrExisting": {
            "type": "String",
            "allowedValues": [
                "new",
                "existing"
            ]
        },
        "applicationStorageAccountName": {
            "defaultValue": "",
            "type": "String"
        },
        "applicationStorageAccountType": {
            "defaultValue": "",
            "type": "String"
        },
        "applicationStorageAccountKind": {
            "defaultValue": "",
            "type": "String"
        },
